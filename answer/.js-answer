  let count = 3;
let message= 'ボブのメッセージ';
let message2 = 'ジェシーのメッセージ';

function messageBox(count, message, message2) {
    let resultWord = '';

    for (let i = 1; i <= count; i++) {
        if (i % 2 === 0) {
            resultWord += message
        } else {
            resultWord += message2;
        }
    }

    return resultWord;
}

console.log(messageBox(count, message, message2));
